    "@nuxtjs/eslint-config": "^6.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^15.12.4",
    "@types/string-similarity": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "prisma": "2.25.0",
